{
  "name": "Inklusio-website",
  "version": "1.0.2",
  "description": "Inklusio websitebuilt on a starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "watch:sass": "sass --watch _includes/sass:css",
    "build:sass": "sass _includes/sass:css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "purgecss": "purgecss --css ./_site/css/my-bootstrap.css --content ./_site/**/*.html --output ./_site/css/my-bootstrap.css",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:* purgecss"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/skotkjerra/Inklusio--Website"
  },
  "author": {
    "name": "Stein Erik Skotkjerra",
    "email": "stein.erik.skotkjerra@gmail.com",
    "url": "inklusio.dk/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "purgecss": "^4.0.3",
    "sass": "^1.32.8"
  }
}
