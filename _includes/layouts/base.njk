<!doctype html>
<html lang="da-DK">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ title }} - Inklusio</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">

    <link rel="stylesheet" href="{{ '/css/my-bootstrap.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">

    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>
  <body>
  <div id="container">
        <header>
<a class="skiptomain" href="#main">Spring til hovedindhold</a>

            <nav class="navbar navbar-expand-lg navbar-dark" aria-label="Top menu">
                <a class="navbar-brand" href="#">
                    <img src="
					{{ '/img/logo.jpg' | url }}" alt="Inklusio logo" width="100" height="30">
                </a>

                <button aria-label="menu" class="navbar-toggler ml-auto custom-toggler" type="button" data-toggle="collapse" data-target="#collapsingNavbar4" aria-expanded="false">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse" id="collapsingNavbar4">
                    <span class="navbar-nav">

      {%- for entry in collections.all | eleventyNavigation %}
        <a class="nav-item nav-link text-white {% if entry.url == page.url %} active {% endif %}" {% if entry.url == page.url %} aria-current="page"{% endif %} href="{{ entry.url | url }}">{{ entry.title }}</a>
      {%- endfor %}
	  		</span>
        </div>
		      </nav>     
        </header>

    <main id="main" tabindex="-1" {% if templateClass %} class="{{ templateClass }}"{% endif %}>

      {{ content | safe }}
    </main>
 </div>
    <div id="Footer" class="footer">
        <footer class="navbar bg-primary">
            <div id="FooterLeftCol" class="FooterFont col-12 col-sm-12 col-md-12  col-lg-4 col-xl-4 ">
                <p>
                    Inklusio ApS, CVR: 41066784
                </p>
            </div>
            <div id="FooterMiddleCol" class="FooterFont col-12 col-sm-12 col-md-12  col-lg-4 col-xl-4 ">
                <p>
                    Engg√•rdsgade 1, 1.th. 9000 Aalborg, Denmark

                </p>
            </div>
            <div id="FooterRightCol" class="FooterFont col-12 col-sm-12 col-md-12  col-lg-4 col-xl-4 ">
                <p>
                    Telefon: +45 26 34 34 55

                </p>
            </div>

        </footer>
    </div>

    <!-- Current page: {{ page.url | url }} -->


<script type="text/javascript">
/*<![CDATA[*/
